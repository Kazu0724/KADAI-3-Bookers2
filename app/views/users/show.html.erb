<div>
  <h1><%= @user %></h1>
  <%= image_tag @user.get_profile_image(100,100) %>
  <p><%= link_to "プロフィール編集", @user %></p>
</div>

<%= @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
    <%= image_tag book.get_image %>
  <% end %>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p><%= book.user.name %></p>
  <p><%= book.title %></p>
  <p><%= book.body %></p>
</div>
<% end %>
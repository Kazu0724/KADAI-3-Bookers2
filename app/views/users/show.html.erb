<h2>User info</h2>
  <%= image_tag @user.get_profile_image(100,100) %>
  <table class="user-info">

  </table>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
</div>

<%= @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
    <%= image_tag book.get_image %>
  <% end %>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p><%= book.user.name %></p>
  <p><%= book.title %></p>
  <p><%= book.body %></p>
<% end %>